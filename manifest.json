{
  "manifest_version": 3,
  "name": "Cookie Security Enhancer",
  "version": "1.0",
  "description": "Scans and reports cookie security attributes.",
  "permissions": [
    "cookies",
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
